{
	"env": {
		"commonjs": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest"
	},
	"plugins": ["react", "prettier"],
	"rules": { "prettier/prettier": "error" },
	"lint": "eslint ./",
	"lintFix": "eslint ./ --fix"
}
